#ifndef __GPIO_DRIVER_H
#define __GPIO_DRIVER_H

#ifdef __cplusplus
 extern "C" {
#endif

#define __REG					*(volatile unsigned int *)

typedef unsigned int UINT32;
typedef unsigned char UINT8;

typedef struct
{
	__REG unsigned int MODE_R;
	__REG unsigned int OTYPE_R;


}GPIO;

typedef enum
{
	LOW = 0,
	HIGH = 1
}GPIO_PinState;



UINT8 initPin();
UINT8 readPin();
UINT8 writePin();





#endif
