#define PERIPH_BASE 			0x40000000UL
#define AHB1PERIPH_BASE 		(PERIPH_BASE + 0x20000UL)

#define GPIOA_BASE				(AHB1PERIPH_BASE + 0x0UL)
#define GPIOA_MODE_R			(*(volatile unsigned int *)(GPIOA_BASE + 0x00UL))
#define GPIOA_OD_R
#define GPIOA_ID_R				(*(volatile unsigned int *)(GPIOA_BASE + 0x10L))

#define GPIOB_BASE				(AHB1PERIPH_BASE + 0x20400UL)
#define GPIOC_BASE				(AHB1PERIPH_BASE + 0x20800UL)

#define GPIOD_BASE				(AHB1PERIPH_BASE + 0x20C00UL)
#define GPIOD_MODE_R			(*(volatile unsigned int *)(GPIOD_BASE + 0x00UL))
#define GPIOD_OD_R				(*(volatile unsigned int *)(GPIOD_BASE + 0x14UL))

#define GPIOE_BASE				(AHB1PERIPH_BASE + 0x21000UL)
#define GPIOF_BASE				(AHB1PERIPH_BASE + 0x21400UL)
#define GPIOG_BASE				(AHB1PERIPH_BASE + 0x21800UL)
#define GPIOH_BASE				(AHB1PERIPH_BASE + 0x21C00UL)
#define GPIOI_BASE				(AHB1PERIPH_BASE + 0x22000UL)
#define GPIOJ_BASE				(AHB1PERIPH_BASE + 0x22400UL)
#define GPIOK_BASE				(AHB1PERIPH_BASE + 0x22800UL)

#define PIN_0
#define PIN_1
#define PIN_2
#define PIN_3
#define PIN_4
#define PIN_5
#define PIN_6
#define PIN_7
#define PIN_8
#define PIN_9
#define PIN_10
#define PIN_11
#define PIN_12
#define PIN_13
#define PIN_14
#define PIN_15

#define RCC_BASE 				(AHB1PERIPH_BASE + 0x3800UL)						// 0x4002 3800
#define RCC_AHB1EN_R			(*(volatile unsigned int *)(RCC_BASE + 0x30UL))		// 0x4002 3830

#define RCC_GPIOA_EN			(1U << 0)
